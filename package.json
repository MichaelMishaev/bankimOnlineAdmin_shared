{
  "name": "bankim-management-portal",
  "version": "1.0.0",
  "type": "module",
  "description": "BankIM Management Portal - современная веб-платформа для управления банковскими операциями",
  "main": "main.tsx",
  "scripts": {
    "start": "node server/server-railway.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:server": "node server/server-local.js",
    "test:config": "node server/test-config-databases.js",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "backend:install": "cd backend && npm install",
    "backend:migrate": "cd backend && npm run db:migrate",
    "backend:status": "cd backend && npm run db:status",
    "backend:test": "cd backend && node test-api.js",
    "setup": "npm install && npm run backend:install && npm run backend:migrate",
    "full-dev": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "test:integration": "npm run backend:migrate && npm run backend:test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:mortgage": "cypress run --spec 'cypress/e2e/mortgage-drill-navigation.cy.ts'",
    "test:content-errors": "cypress run --spec 'cypress/e2e/content-not-found-check.cy.ts'",
    "test:full-drill": "cypress run --spec 'cypress/e2e/full-drill-depth-test.cy.ts'",
    "test:all": "cypress run"
  },
  "dependencies": {
    "@figma/code-connect": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "puppeteer": "^24.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "author": "Michael Mishayev",
  "license": "MIT"
}
